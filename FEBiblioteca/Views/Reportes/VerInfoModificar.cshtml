@model FEBiblioteca.Models.ReportesModel

@{
    ViewData["Title"] = "VerInfoModificar";
}

<h1>Detalle reporte</h1>

<h4>Reportes</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="VerInfoModificar">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="id_reporte" class="control-label">ID Reporte</label>
                <input asp-for="id_reporte" readonly="readonly" class="form-control" />
                <span asp-validation-for="id_reporte" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Reporte" class="control-label"></label>
                <input asp-for="Reporte" readonly="readonly" class="form-control" />
                <span asp-validation-for="Reporte" class="text-danger"></span>
            </div>
         
        </form>
    </div>
</div>

<div>
    <a class="btn btn-warning" asp-action="Index">Volver a Lista</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
